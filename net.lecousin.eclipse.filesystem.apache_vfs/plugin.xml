<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.core.filesystem.filesystems">
      <filesystem
            scheme="http">
         <run
               class="net.lecousin.eclipse.filesystem.apache_vfs.VFSFileSystem">
         </run>
      </filesystem>
   </extension>
   <extension
         point="org.eclipse.core.filesystem.filesystems">
      <filesystem
            scheme="https">
         <run
               class="net.lecousin.eclipse.filesystem.apache_vfs.VFSFileSystem">
         </run>
      </filesystem>
   </extension>
   <extension
         point="org.eclipse.core.filesystem.filesystems">
      <filesystem
            scheme="ftp">
         <run
               class="net.lecousin.eclipse.filesystem.apache_vfs.VFSFileSystem">
         </run>
      </filesystem>
   </extension>

</plugin>
